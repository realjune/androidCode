<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wu.a.template"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="wu.a.template.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="wu.a.template.db.DBProvider"
            android:authorities="wu.a.template.providder" />
        
        <!-- aidl -->
        <service android:name="wu.a.template.aidl.RemoteMediaPlayerService" android:process=":remote">
            <intent-filter> 
                <action android:name="wu.a.template.aidl.RemoteMediaPlayerService" /> 
        </intent-filter> 
        </service>
    </application>

</manifest>