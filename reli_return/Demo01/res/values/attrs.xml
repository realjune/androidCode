<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="FlipViewController">
		<attr name="orientation" format="enum">
			<enum name="vertical_top" value="0"/>
			<enum name="vertical_bottom" value="1"/>
			<enum name="horizontal_left" value="2"/>
			<enum name="horizontal_right" value="3"/>
		</attr>

		<attr name="animationBitmapFormat" format="enum">
			<enum name="ARGB_8888" value="0"/>
			<enum name="ARGB_4444" value="1"/>
			<enum name="RGB_565" value="2"/>
		</attr>
	</declare-styleable>

</resources>