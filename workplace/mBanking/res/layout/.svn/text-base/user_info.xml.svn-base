<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/background"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_gravity="center_vertical"
        android:background="@drawable/background"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/head"
            style="@style/userinfo_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:text="@string/user_info" />

        <TextView
            android:id="@+id/time"
            style="@style/userinfo_hint_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginBottom="10dp"
            android:text="@string/last_update_on" />
    </LinearLayout>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true" >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/contactLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="10dp"
                    android:gravity="clip_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imageItems"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btn_showitems" />

                    <ImageView
                        android:id="@+id/imageSHint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignRight="@+id/imageItems"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btnarrow_down" />

                    <TextView
                        style="@style/userinfo_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/imageItems"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="55dp"
                        android:text="@string/show_items" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="clip_vertical" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/searchLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:gravity="clip_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imagePin"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btn_pinmanager" />

                    <ImageView
                        android:id="@+id/imagePHint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignRight="@+id/imagePin"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btnarrow_down" />

                    <TextView
                        style="@style/userinfo_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/imagePin"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="55dp"
                        android:text="@string/pin_manger" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="clip_vertical" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/accountLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:gravity="clip_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imageAccount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btn_myaccounts" />

                    <ImageView
                        android:id="@+id/imageMAHint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignRight="@+id/imageAccount"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btnarrow_down" />

                    <TextView
                        style="@style/userinfo_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/imageAccount"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="55dp"
                        android:text="@string/my_account" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="clip_vertical" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/cardLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:gravity="clip_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imageCards"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btn_mycards" />

                    <ImageView
                        android:id="@+id/imageMCHint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignRight="@+id/imageCards"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btnarrow_down" />

                    <TextView
                        style="@style/userinfo_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/imageCards"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="55dp"
                        android:text="@string/my_cards" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="clip_vertical" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/loansLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:gravity="clip_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imageLoans"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btn_myloans" />

                    <ImageView
                        android:id="@+id/imageMLHint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignRight="@+id/imageLoans"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btnarrow_down" />

                    <TextView
                        style="@style/userinfo_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/imageLoans"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="55dp"
                        android:text="@string/my_loans" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="clip_vertical" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/investmentsLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:gravity="clip_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imageInvest"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btn_myinvestments" />

                    <ImageView
                        android:id="@+id/imageMIHint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignRight="@+id/imageInvest"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btnarrow_down" />

                    <TextView
                        style="@style/userinfo_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/imageInvest"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="55dp"
                        android:text="@string/my_invest" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="clip_vertical" />
            </LinearLayout>
            
            <LinearLayout
                android:id="@+id/pushsettingsLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp"
                    android:layout_marginTop="5dp"
                    android:gravity="clip_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imagePush"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="center_horizontal"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btn_notification" />

                    <ImageView
                        android:id="@+id/imagePushDown"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignRight="@+id/imagePush"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:contentDescription="@string/app_name"
                        android:src="@drawable/btnarrow_down" />

                    <TextView
                        style="@style/userinfo_text_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignLeft="@+id/imagePush"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="55dp"
                        android:text="@string/my_push" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:gravity="clip_vertical" />
            </LinearLayout>
        </LinearLayout>

    </ScrollView>

</LinearLayout>